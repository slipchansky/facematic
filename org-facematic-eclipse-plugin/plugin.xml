<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="Facematic  Wizards"
            id="org.facematic">
      </category>
      <wizard
            category="org.facematic"
            class="org.facematic.wizards.FacematicNewWizard"
            icon="icons/fm-vaadin.png"
            id="org.facematic.wizards.FacematicNewWizard"
            name="Create Facematic View+Controller">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.facematic.facematic.editors.FmMvcEditor"
            contributorClass="org.facematic.facematic.editors.FmMvcEditorContributor"
            extensions="xml, java"
            icon="icons/fm-vaadin.png"
            id="org.facematic.facematic.editors.FmMvcEditor"
            name="Facematic View+Controller editor">
      </editor>
   </extension>
   
   
</plugin>
