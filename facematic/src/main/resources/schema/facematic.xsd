					use="optional">
				</attribute>
				<attribute name="newItemsAllowed" type="c:TrueFalseType"
					use="optional">
				</attribute>
				<attribute name="selectable" type="c:TrueFalseType" use="optional">
				</attribute>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="SubstituteType" mixed="true">
