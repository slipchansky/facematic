					</element>
					<element type="c:ContentContainerType" name="hl" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:ContentContainerType" name="vl" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:ButtonType" name="Button" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:FieldType" name="TextField" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:LabelType" name="Label" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:LinkType" name="Link" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:EmbeddedType" name="Embedded" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:PopupDateFieldType" name="PopupDateField"
						minOccurs="0" maxOccurs="unbounded">
					</element>
					<element type="c:InlineDateFieldType" name="InlineDateField"
						minOccurs="0" maxOccurs="unbounded">
					</element>
					<element type="c:TextAreaType" name="TextArea" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:RichTextAreaType" name="RichTextArea"
						minOccurs="0" maxOccurs="unbounded">
					</element>
					<element type="c:ListSelectType" name="ListSelect"
						minOccurs="0" maxOccurs="unbounded">
					</element>

					<element type="c:FieldType" name="CheckBox" minOccurs="0"
						maxOccurs="unbounded" />

					<element name="ComboBox" type="c:ComboBoxType" maxOccurs="unbounded"
						minOccurs="0">
					</element>
					<element type="c:TwinColSelectType" name="TwinColSelect"
						minOccurs="0" maxOccurs="unbounded">
					</element>
					<element type="c:OptionGroupType" name="OptionGroup"
						minOccurs="0" maxOccurs="unbounded">
					</element>
					<element type="c:SliderType" name="Slider" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:UploadType" name="Upload" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:FormLayoutType" name="FormLayout"
						minOccurs="0" maxOccurs="unbounded">
					</element>
					<element type="c:TableType" name="Table" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:TreeType" name="Tree" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:PanelType" name="Panel" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:CompositeType" name="composite" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:PlaceHolderType" name="placeholder"
						minOccurs="0" maxOccurs="unbounded" />
				</sequence>
				<attribute name="spacing" type="c:TrueFalseType"></attribute>
				<attribute name="margin" type="c:TrueFalseType"></attribute>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="TabSheetType">
		<complexContent>
			<extension base="c:ContentContainerType">
				<attribute name="hideTabs" type="c:TrueFalseType"></attribute>
			</extension>
		</complexContent>
	</complexType>


	<complexType name="CoreHtmlType" mixed="true">
