			<element type="c:CoreHtmlTableType" name="table" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="p" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="span" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="div" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="input" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="a" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="b" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="br" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="button" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="cente" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="form" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="li" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlImageType" name="image" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlImageType" name="img" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlIframeType" name="iframe" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="i" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="hr" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="ul" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="pre" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="select" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="option" minOccurs="0"
				maxOccurs="unbounded" />

			<element type="c:CoreHtmlType" name="textarea" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="h1" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="h2" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="h3" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="h4" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="h5" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="h6" minOccurs="0"
				maxOccurs="unbounded" />
			<element type="c:CoreHtmlType" name="font" minOccurs="0"
				maxOccurs="unbounded" />
		</sequence>
		<attribute name="class" type="string"></attribute>
		<attribute name="style" type="string"></attribute>
		<attribute name="width" type="string"></attribute>
		<attribute name="height" type="string"></attribute>
		<attribute name="name" type="string"></attribute>
		<attribute name="id" type="string"></attribute>
		<attribute name="type" type="string"></attribute>
		<attribute name="align" type="string"></attribute>
		<attribute name="size" type="string"></attribute>
	</complexType>


	<complexType name="CoreHtmlTdType">
		<complexContent>
			<extension base="c:CoreHtmlType">
				<attribute name="colspan" type="int"></attribute>
				<attribute name="rowspan" type="int"></attribute>
				<attribute name="valign" type="string"></attribute>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="CoreHtmlTrType">
		<sequence minOccurs="1" maxOccurs="unbounded">
			<element type="c:CoreHtmlTdType" name="td" minOccurs="1"
				maxOccurs="unbounded" />
		</sequence>
		<attribute name="valign" type="string"></attribute>
		<attribute name="height" type="string"></attribute>
		<attribute name="colspan" type="int"></attribute>
		<attribute name="rowspan" type="int"></attribute>
	</complexType>

	<complexType name="CoreHtmlImageType">
		<complexContent>
			<extension base="c:CoreHtmlType">
				<attribute name="src" type="string"></attribute>
			</extension>
		</complexContent>
	</complexType>


	<complexType name="CoreHtmlTableType">
		<sequence minOccurs="1" maxOccurs="unbounded">
			<element type="c:CoreHtmlTrType" name="tr" minOccurs="1"
				maxOccurs="unbounded" />
		</sequence>
		<attribute name="border" type="int"></attribute>
		<attribute name="width" type="string"></attribute>
		<attribute name="height" type="string"></attribute>
		<attribute name="style" type="string"></attribute>
		<attribute name="id" type="string"></attribute>
		<attribute name="cellspacing" type="string"></attribute>
		<attribute name="cellpadding" type="string"></attribute>
		<attribute name="bgcolor" type="string"></attribute>
		<attribute name="frame" type="string"></attribute>
	</complexType>

	<complexType name="CoreHtmlIframeType">
		<complexContent>
			<extension base="c:CoreHtmlType">
				<attribute name="src" type="string"></attribute>
				<attribute name="frameborder" type="int"></attribute>
				<attribute name="allowfullscreen" type="int"></attribute>
			</extension>
		</complexContent>
	</complexType>





	<complexType name="HtmlType" mixed="true">
		<complexContent>
			<extension base="c:LabelType">
				<sequence maxOccurs="unbounded" minOccurs="0">
					<element type="c:CoreHtmlTableType" name="table" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="p" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="span" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="div" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="input" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="a" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="b" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="br" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="button" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="cente" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="form" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="li" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlImageType" name="img" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlIframeType" name="iframe"
						minOccurs="0" maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="i" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="hr" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="ul" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="pre" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="select" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="option" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="textarea" minOccurs="0"
						maxOccurs="unbounded" />

					<element type="c:CoreHtmlType" name="h1" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="h2" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="h3" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="h4" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="h5" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="h6" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:CoreHtmlType" name="font" minOccurs="0"
						maxOccurs="unbounded" />

				</sequence>
			</extension>
		</complexContent>
	</complexType>




	<complexType name="ContentType">
		<sequence>
		</sequence>
		<attribute name="name" type="string" use="optional"></attribute>
		<attribute name="id" type="string" use="optional"></attribute>
		<attribute name="caption" type="string" use="optional"></attribute>
		<attribute name="height" type="string" use="optional"></attribute>
		<attribute name="width" type="string" use="optional"></attribute>
		<attribute name="alignment" type="c:ContentTypeAlignmentType"
			use="optional" />
		<attribute name="sizeFull" type="c:TrueFalseType" use="optional"></attribute>
		<attribute name="expandRatio" type="float" use="optional" />
		<attribute name="enabled" type="c:TrueFalseType" use="optional"></attribute>
		<attribute name="visible" type="c:TrueFalseType" use="optional"></attribute>
		<attribute name="styleName" type="string" use="optional"></attribute>
		<attribute name="immediate" type="c:TrueFalseType" use="optional"></attribute>
		<attribute name="description" type="string" use="optional"></attribute>
		<attribute name="class" type="string" use="optional">
			<annotation>
				<documentation>
					Qualified component class name, for example,
					name="com.vaadin.ui.Button"
				</documentation>
			</annotation>
		</attribute>
	</complexType>


	<simpleType name="ContentTypeAlignmentType">
		<restriction base="string">
			<enumeration value="BOTTOM_CENTER" />
			<enumeration value="BOTTOM_LEFT" />
			<enumeration value="BOTTOM_RIGHT" />
			<enumeration value="MIDDLE_CENTER" />
			<enumeration value="MIDDLE_LEFT" />
			<enumeration value="MIDDLE_RIGHT" />
			<enumeration value="TOP_CENTER" />
			<enumeration value="TOP_LEFT" />
			<enumeration value="TOP_RIGHT" />
		</restriction>
	</simpleType>

	<simpleType name="TrueFalseType">
		<restriction base="string">
			<enumeration value="false" />
			<enumeration value="true" />
		</restriction>
	</simpleType>




	<complexType name="PanelType">
		<complexContent>
			<extension base="c:ContentType">
				<choice minOccurs="1" maxOccurs="1">
					<element type="c:TabSheetType" name="TabSheet" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:ContentContainerType" name="hl" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:ContentContainerType" name="vl" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:ButtonType" name="Button" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:FieldType" name="TextField" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:LabelType" name="Label" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:HtmlType" name="Html" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:HtmlType" name="html" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:LinkType" name="Link" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:EmbeddedType" name="Embedded" minOccurs="0"
						maxOccurs="unbounded" />
					<element type="c:PopupDateFieldType" name="PopupDateField"
						minOccurs="0" maxOccurs="unbounded">
					</element>
					<element type="c:InlineDateFieldType" name="InlineDateField"
						minOccurs="0" maxOccurs="unbounded">
					</element>
					<element type="c:TextAreaType" name="TextArea" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:RichTextAreaType" name="RichTextArea"
						minOccurs="0" maxOccurs="unbounded">
					</element>
					<element type="c:ListSelectType" name="ListSelect"
						minOccurs="0" maxOccurs="unbounded">
					</element>
					<element name="ComboBox" type="c:ComboBoxType" maxOccurs="unbounded"
						minOccurs="0">
					</element>
					<element type="c:TwinColSelectType" name="TwinColSelect"
						minOccurs="0" maxOccurs="unbounded">
					</element>
					<element type="c:OptionGroupType" name="OptionGroup"
						minOccurs="0" maxOccurs="unbounded">
					</element>
					<element type="c:SliderType" name="Slider" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:UploadType" name="Upload" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:FormLayoutType" name="FormLayout"
						minOccurs="0" maxOccurs="unbounded">
					</element>
					<element type="c:TableType" name="Table" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:TreeType" name="Tree" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:PanelType" name="Panel" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:CompositeType" name="composite" minOccurs="0"
						maxOccurs="unbounded">
					</element>
					<element type="c:PlaceHolderType" name="placeholder"
						minOccurs="0" maxOccurs="unbounded" />
				</choice>
			</extension>
		</complexContent>
	</complexType>

	<simpleType name="CompositeLocationType">
		<restriction base="string">
			<enumeration value="RESOURCE" />
			<enumeration value="FILE" />
			<enumeration value="URL" />
		</restriction>
	</simpleType>

	<complexType name="CompositeType">
		<complexContent>
			<extension base="c:ContentType">
				<attribute name="controller" type="string" use="optional"></attribute>
				<attribute name="location" type="c:CompositeLocationType"
					use="required"></attribute>
				<attribute name="path" type="string" use="required"></attribute>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="ExtensionCompositeType">
		<complexContent>
			<extension base="c:CompositeType">
				<attribute name="placeholder" type="string" use="required"></attribute>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PlaceHolderType">
		<complexContent>
			<extension base="c:ContentType">
				<attribute name="controller" type="string" use="optional"></attribute>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="EventListenerType" mixed="true">
		<attribute name="call" type="string"></attribute>
	</complexType>


	<complexType name="PropertyType">
		<complexContent>
			<extension base="c:ContentType">
				<attribute name="value" type="string"></attribute>
				<attribute name="onChange" type="string">
					<annotation>
						<documentation>
							The name of controller method. You must implement
							method with this
							name with parameter type of ValueChangeEvent. for
							example: public
							void onValueChange (ValueChangeEvent event) {...}.
						</documentation>
					</annotation>
				</attribute>
			</extension>

		</complexContent>
	</complexType>

	<complexType name="FieldType">
		<complexContent>
			<extension base="c:PropertyType">
				<attribute name="inputPrompt" use="optional" type="string"></attribute>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="TextFieldType">
		<complexContent>
			<extension base="c:FieldType">
			</extension>
		</complexContent>
	</complexType>


	<simpleType name="PopupDateFieldResolutionType">
		<restriction base="string">
			<enumeration value="SECOND" />
			<enumeration value="MINUTE" />
			<enumeration value="HOUR" />
			<enumeration value="DAY" />
			<enumeration value="MONTH" />
			<enumeration value="YEAR" />
		</restriction>
	</simpleType>



	<complexType name="PopupDateFieldType">
		<complexContent>
			<extension base="c:FieldType">
				<attribute name="resolution" type="c:PopupDateFieldResolutionType" />
			</extension>
		</complexContent>
	</complexType>

	<complexType name="InlineDateFieldType">
		<complexContent>
			<extension base="c:PopupDateFieldType" />
		</complexContent>
	</complexType>



	<complexType name="LabelType">
		<complexContent>
			<extension base="c:PropertyType" />
		</complexContent>
	</complexType>

	<complexType name="LinkType">
		<complexContent>
			<extension base="c:PropertyType">
				<attribute name="url" type="string"></attribute>
				<attribute name="resource" type="string"></attribute>
			</extension>
		</complexContent>
	</complexType>


	<simpleType name="EmbeddedTypeType">
		<restriction base="string">
			<enumeration value="TYPE_BROWSER" />
			<enumeration value="TYPE_IMAGE" />
			<enumeration value="TYPE_OBJECT" />
		</restriction>
	</simpleType>


	<complexType name="EmbeddedType">
		<complexContent>
			<extension base="c:LinkType">
				<sequence>
					<element name="parameter" type="string" minOccurs="0"
						maxOccurs="unbounded" />
				</sequence>

				<attribute name="alternateText" type="string" />
				<attribute name="mimeType" type="string" />
				<attribute name="type" type="c:EmbeddedTypeType" />
			</extension>
		</complexContent>
	</complexType>


	<complexType name="TextAreaType">
		<complexContent>
			<extension base="c:FieldType">
				<attribute name="rows" type="int" />
				<attribute name="columns" type="int" />
			</extension>
		</complexContent>
	</complexType>

	<complexType name="RichTextAreaType">
		<complexContent>
			<extension base="c:TextAreaType">
			</extension>
		</complexContent>
	</complexType>



	<complexType name="ButtonType">
		<complexContent>
			<extension base="c:ContentType">
				<attribute name="onClick" type="string">
					<annotation>
						<documentation>
							The name of controller method. You must implement
							method with such
							name in your controller. For example: public void
							onClick
							(ClickEvent event) {...}.
						</documentation>
					</annotation>
				</attribute>

			</extension>
		</complexContent>
	</complexType>

	<complexType name="ItemType" mixed="true">
		<sequence>
			<element name="item" type="c:ItemType" maxOccurs="unbounded"
				minOccurs="0" />
		</sequence>
		<attribute name="value" use="optional"></attribute>
	</complexType>


	<complexType name="ListSelectType">
		<complexContent>
			<extension base="c:FieldType">
				<sequence>
					<element name="item" type="c:ItemType" maxOccurs="unbounded"
						minOccurs="0" />
				</sequence>
				<attribute name="enum" type="string"></attribute>
				<attribute name="rows" type="integer" use="optional"></attribute>
				<attribute name="nullSelectionAllowed" type="c:TrueFalseType"
					use="optional"></attribute>
				<attribute name="multiSelect" type="c:TrueFalseType"></attribute>
			</extension>
		</complexContent>
	</complexType>



	<complexType name="TwinColSelectType">
		<complexContent>
			<extension base="c:ListSelectType">
				<attribute name="leftColumnCaption" type="string" use="optional"></attribute>
				<attribute name="rightColumnCaption" type="string" use="optional"></attribute>
			</extension>
		</complexContent>
	</complexType>




	<complexType name="OptionGroupType">
		<complexContent>
			<extension base="c:ListSelectType">
				<attribute name="htmlContentAllowed" type="c:TrueFalseType"
